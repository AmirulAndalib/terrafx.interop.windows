<doc>
  <member name="ID3D10EffectRenderTargetViewVariable.GetRenderTarget">
    <summary>
      <para>Get a render-target.</para>
    </summary>
    <param name="ppResource">
      <para>Type: <b><see cref="ID3D10RenderTargetView" />**</b></para>
      <para>The address of a pointer to a render-target-view interface. See <see cref="ID3D10RenderTargetView Interface" />.</para>
    </param>
    <returns>
      <para>Type: <b><a href="https://docs.microsoft.com//windows/win32/com/structure-of-com-error-codes">HRESULT</a></b></para>
      <para>Returns one of the following <a href="https://docs.microsoft.com//windows/desktop/direct3d10/d3d10-graphics-reference-returnvalues">Direct3D 10 Return Codes</a>.</para>
    </returns>
    <seealso cref="ID3D10EffectRenderTargetViewVariable Interface" />
  </member>
  <member name="ID3D10EffectRenderTargetViewVariable.GetRenderTargetArray">
    <summary>
      <para>Get an array of render-targets.</para>
    </summary>
    <param name="ppResources">
      <para>Type: <b><see cref="ID3D10RenderTargetView" />**</b></para>
      <para>A pointer to an array of render-target-view interfaces. See <see cref="ID3D10RenderTargetView Interface" />.</para>
    </param>
    <param name="Offset">
      <para>Type: <b><a href="https://docs.microsoft.com//windows/desktop/WinProg/windows-data-types">UINT</a></b></para>
      <para>The zero-based array index to get the first interface.</para>
    </param>
    <param name="Count">
      <para>Type: <b><a href="https://docs.microsoft.com//windows/desktop/WinProg/windows-data-types">UINT</a></b></para>
      <para>The number of elements in the array.</para>
    </param>
    <returns>
      <para>Type: <b><a href="https://docs.microsoft.com//windows/win32/com/structure-of-com-error-codes">HRESULT</a></b></para>
      <para>Returns one of the following <a href="https://docs.microsoft.com//windows/desktop/direct3d10/d3d10-graphics-reference-returnvalues">Direct3D 10 Return Codes</a>.</para>
    </returns>
    <seealso cref="ID3D10EffectRenderTargetViewVariable Interface" />
  </member>
  <member name="ID3D10EffectRenderTargetViewVariable.SetRenderTarget">
    <summary>
      <para>Set a render-target.</para>
    </summary>
    <param name="pResource">
      <para>Type: <b><see cref="ID3D10RenderTargetView" />*</b></para>
      <para>A pointer to a render-target-view interface. See <see cref="ID3D10RenderTargetView Interface" />.</para>
    </param>
    <returns>
      <para>Type: <b><a href="https://docs.microsoft.com//windows/win32/com/structure-of-com-error-codes">HRESULT</a></b></para>
