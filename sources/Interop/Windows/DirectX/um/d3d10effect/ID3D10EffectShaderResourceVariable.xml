<doc>
  <member name="ID3D10EffectShaderResourceVariable.GetResource">
    <summary>
      <para>Get a shader resource.</para>
    </summary>
    <param name="ppResource">
      <para>Type: <b><see cref="ID3D10ShaderResourceView" />**</b></para>
      <para>The address of a pointer to a shader-resource-view interface. See <see cref="ID3D10ShaderResourceView Interface" />.</para>
    </param>
    <returns>
      <para>Type: <b><a href="https://docs.microsoft.com//windows/win32/com/structure-of-com-error-codes">HRESULT</a></b></para>
      <para>Returns one of the following <a href="https://docs.microsoft.com//windows/desktop/direct3d10/d3d10-graphics-reference-returnvalues">Direct3D 10 Return Codes</a>.</para>
    </returns>
    <seealso cref="ID3D10EffectShaderResourceVariable Interface" />
  </member>
  <member name="ID3D10EffectShaderResourceVariable.GetResourceArray">
    <summary>
      <para>Get an array of shader resources.</para>
    </summary>
    <param name="ppResources">
      <para>Type: <b><see cref="ID3D10ShaderResourceView" />**</b></para>
      <para>The address of an array of shader-resource-view interfaces. See <see cref="ID3D10ShaderResourceView Interface" />.</para>
    </param>
    <param name="Offset">
      <para>Type: <b><a href="https://docs.microsoft.com//windows/desktop/WinProg/windows-data-types">UINT</a></b></para>
      <para>The zero-based array index to get the first interface.</para>
    </param>
    <param name="Count">
      <para>Type: <b><a href="https://docs.microsoft.com//windows/desktop/WinProg/windows-data-types">UINT</a></b></para>
      <para>The number of elements in the array.</para>
    </param>
    <returns>
      <para>Type: <b><a href="https://docs.microsoft.com//windows/win32/com/structure-of-com-error-codes">HRESULT</a></b></para>
      <para>Returns one of the following <a href="https://docs.microsoft.com//windows/desktop/direct3d10/d3d10-graphics-reference-returnvalues">Direct3D 10 Return Codes</a>.</para>
    </returns>
    <seealso cref="ID3D10EffectShaderResourceVariable Interface" />
  </member>
  <member name="ID3D10EffectShaderResourceVariable.SetResource">
    <summary>
      <para>Set a shader resource.</para>
    </summary>
    <param name="pResource">
      <para>Type: <b><see cref="ID3D10ShaderResourceView" />*</b></para>
      <para>The address of a pointer to a shader-resource-view interface. See <see cref="ID3D10ShaderResourceView Interface" />.</para>
    </param>
    <returns>
      <para>Type: <b><a href="https://docs.microsoft.com//windows/win32/com/structure-of-com-error-codes">HRESULT</a></b></para>
      <para>Returns one of the following <a href="https://docs.microsoft.com//windows/desktop/direct3d10/d3d10-graphics-reference-returnvalues">Direct3D 10 Return Codes</a>.</para>
    </returns>
    <seealso cref="ID3D10EffectShaderResourceVariable Interface" />
  </member>
  <member name="ID3D10EffectShaderResourceVariable.SetResourceArray">
    <summary>
      <para>Set an array of shader resources.</para>
    </summary>
    <param name="ppResources">
      <para>Type: <b><see cref="ID3D10ShaderResourceView" />**</b></para>
      <para>The address of an array of shader-resource-view interfaces. See <see cref="ID3D10ShaderResourceView Interface" />.</para>
    </param>
    <param name="Offset">
      <para>Type: <b><a href="https://docs.microsoft.com//windows/desktop/WinProg/windows-data-types">UINT</a></b></para>
