<doc>
  <member name="COAUTHIDENTITY">
    <summary>Contains a user name and password.</summary>
  </member>
  <member name="COAUTHIDENTITY.User">
    <summary>The user's name.</summary>
  </member>
  <member name="COAUTHIDENTITY.UserLength">
    <summary>The length of the <b>User</b> string, without the terminating <b>NULL</b>.</summary>
  </member>
  <member name="COAUTHIDENTITY.Domain">
    <summary>The domain or workgroup name.</summary>
  </member>
  <member name="COAUTHIDENTITY.DomainLength">
    <summary>The length of the <b>Domain</b> string, without the terminating <b>NULL</b>.</summary>
  </member>
  <member name="COAUTHIDENTITY.Password">
    <summary>The user's password in the domain or workgroup.</summary>
  </member>
  <member name="COAUTHIDENTITY.PasswordLength">
    <summary>The length of the <b>Password</b> string, without the terminating <b>NULL</b>.</summary>
  </member>
  <member name="COAUTHIDENTITY.Flags">
    <summary>
      <para>Indicates whether the strings are Unicode strings.</para>
      <list type="table">
        <listheader>
          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>
                  <b>SEC_WINNT_AUTH_IDENTITY_ANSI</b>
                </term>
              </listheader>
              <listheader>
                <term>0x1</term>
              </listheader>
            </list>
          </description>
          <description>The strings are ANSI strings.</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>
                  <b>SEC_WINNT_AUTH_IDENTITY_UNICODE</b>
                </term>
              </listheader>
              <listheader>
                <term>0x2</term>
              </listheader>
            </list>
          </description>
          <description>The strings are Unicode strings.</description>
        </listheader>
      </list>
    </summary>
  </member>
</doc>