          </description>
        </item>
        <item>
          <description>
            <list type="bullet">
              <item>
                <description>
                  <b>MIB_TCP_RTO_RSRE</b>
                </description>
              </item>
              <item>
                <description>3</description>
              </item>
            </list>
          </description>
