          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <item>
          <description>
            <list type="bullet">
              <item>
                <description>
                  <b>MIB_TCP_RTO_OTHER</b>
                </description>
              </item>
              <item>
                <description>1</description>
              </item>
