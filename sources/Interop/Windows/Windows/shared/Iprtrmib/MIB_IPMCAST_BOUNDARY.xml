          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <item>
          <description>
            <para>1</para>
          </description>
          <description>
            <para>The entry has <b>active</b> status.</para>
          </description>
        </item>
        <item>
          <description>
            <para>2</para>
          </description>
          <description>
            <para>The entry has <b>notInService</b> status.</para>
