<doc>
  <member name="CACHE_DESCRIPTOR">
    <summary>
      <para>Describes the cache attributes.</para>
    </summary>
    <seealso cref="GetLogicalProcessorInformation" />
    <seealso cref="PROCESSOR_CACHE_TYPE" />
    <seealso cref="SYSTEM_LOGICAL_PROCESSOR_INFORMATION" />
  </member>
  <member name="CACHE_DESCRIPTOR.Level">
    <summary>
      <para>The cache level. This member can currently be one of the following values; other values may be supported in the future.</para>
      <list type="table">
        <listheader>
          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <item>
          <description>
            <para>1</para>
          </description>
          <description>
            <para>L1</para>
          </description>
        </item>
        <item>
          <description>
            <para>2</para>
          </description>
          <description>
            <para>L2</para>
          </description>
        </item>
        <item>
          <description>
            <para>3</para>
          </description>
          <description>
            <para>L3</para>
          </description>
        </item>
      </list>
    </summary>
  </member>
  <member name="CACHE_DESCRIPTOR.Associativity">
    <summary>
      <para>The cache associativity. If this member is CACHE_FULLY_ASSOCIATIVE (0xFF), the cache is fully associative.</para>
    </summary>
  </member>
  <member name="CACHE_DESCRIPTOR.LineSize">
    <summary>
      <para>The cache line size, in bytes.</para>
    </summary>
  </member>
  <member name="CACHE_DESCRIPTOR.Size">
    <summary>
      <para>The cache size, in bytes.</para>
    </summary>
  </member>
  <member name="CACHE_DESCRIPTOR.Type">
    <summary>
      <para>The cache type. This member is a <see cref="PROCESSOR_CACHE_TYPE" /> value.</para>
    </summary>
  </member>
</doc>