  <member name="CACHE_DESCRIPTOR.Level">
    <summary>
      <para>The cache level. This member can currently be one of the following values; other values may be supported in the future.</para>
      <list type="table">
        <listheader>
          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>1</term>
              </listheader>
            </list>
          </description>
          <description>L1</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>2</term>
              </listheader>
            </list>
          </description>
          <description>L2</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>3</term>
              </listheader>
            </list>
          </description>
          <description>L3</description>
        </listheader>
      </list>
    </summary>
  </member>
  <member name="CACHE_DESCRIPTOR.Associativity">
    <summary>The cache associativity. If this member is CACHE_FULLY_ASSOCIATIVE (0xFF), the cache is fully associative.</summary>
  </member>
  <member name="CACHE_DESCRIPTOR.LineSize">
    <summary>The cache line size, in bytes.</summary>
  </member>
  <member name="CACHE_DESCRIPTOR.Size">
    <summary>The cache size, in bytes.</summary>
  </member>
  <member name="CACHE_DESCRIPTOR.Type">
