<doc>
  <member name="CATEGORYINFO">
    <summary>
      <para>Describes a component category.</para>
    </summary>
    <seealso cref="ICatRegister" />
  </member>
  <member name="CATEGORYINFO.catid">
    <summary>
      <para>The category identifier for the component.</para>
    </summary>
  </member>
  <member name="CATEGORYINFO.lcid">
    <summary>
