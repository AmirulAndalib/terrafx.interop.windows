    <summary>
      <para>Not used.</para>
    </summary>
  </member>
  <member name="DEVICE_MANAGE_DATA_SET_ATTRIBUTES_OUTPUT.OperationStatus">
    <summary>
      <para>Not used.</para>
    </summary>
  </member>
