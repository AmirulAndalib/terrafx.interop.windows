      <list type="table">
        <listheader>
          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <item>
          <description>
            <b>CSV_NAMESPACE_INFO_V1</b>
          </description>
          <description>
            <para>Version 1.</para>
          </description>
        </item>
      </list>
    </summary>
  </member>
  <member name="CSV_NAMESPACE_INFO.DeviceNumber">
    <summary>
      <para>The device number of the disk.</para>
    </summary>
  </member>
  <member name="CSV_NAMESPACE_INFO.StartingOffset">
