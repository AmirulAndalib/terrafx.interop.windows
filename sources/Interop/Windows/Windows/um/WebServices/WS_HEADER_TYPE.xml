<doc>
  <member name="WS_HEADER_TYPE">
    <summary>
      <para>Identifies a type of header.</para>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_ACTION_HEADER">
    <summary>
      <para>The Action addressing header.</para>
      <para>This header can be used with the following <see cref="WS_TYPE" />s:</para>
      <list type="bullet">
        <item>
          <description>
            <see cref="WS_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_XML_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_WSZ_TYPE" />
          </description>
        </item>
      </list>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_TO_HEADER">
    <summary>
      <para>The To addressing header.</para>
      <para>This header can be used with the following <see cref="WS_TYPE" />s:</para>
      <list type="bullet">
        <item>
          <description>
            <see cref="WS_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_XML_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_WSZ_TYPE" />
          </description>
        </item>
      </list>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_MESSAGE_ID_HEADER">
    <summary>
      <para>The MessageID addressing header.</para>
      <para>This header can be used with the following <see cref="WS_TYPE" />s:</para>
      <list type="bullet">
        <item>
          <description>
            <see cref="WS_UNIQUE_ID_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_XML_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_WSZ_TYPE" />
          </description>
        </item>
      </list>
      <para>This header is not supported for <see cref="WS_ADDRESSING_VERSION_TRANSPORT" />.</para>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_RELATES_TO_HEADER">
    <summary>
      <para>The RelatesTo addressing header.</para>
      <para>This header can be used with the following <see cref="WS_TYPE" />s:</para>
      <list type="bullet">
        <item>
          <description>
            <see cref="WS_UNIQUE_ID_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_XML_STRING_TYPE" />
          </description>
        </item>
        <item>
          <description>
            <see cref="WS_WSZ_TYPE" />
          </description>
        </item>
      </list>
      <para>This header is not supported for <see cref="WS_ADDRESSING_VERSION_TRANSPORT" />.</para>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_FROM_HEADER">
    <summary>
      <para>The From addressing header.</para>
      <para>This header is used with <see cref="WS_ENDPOINT_ADDRESS_TYPE" />.</para>
      <para>This header is not supported for <see cref="WS_ADDRESSING_VERSION_TRANSPORT" />.</para>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_REPLY_TO_HEADER">
    <summary>
      <para>The ReplyTo addressing header.</para>
      <para>This header is used with <see cref="WS_ENDPOINT_ADDRESS_TYPE" />.</para>
      <para>This header is not supported for <see cref="WS_ADDRESSING_VERSION_TRANSPORT" />.</para>
    </summary>
  </member>
  <member name="WS_HEADER_TYPE.WS_FAULT_TO_HEADER">
    <summary>
      <para>The FaultTo addressing header, in <see cref="WS_ENDPOINT_ADDRESS" /> format.</para>
      <para>This header is used with <see cref="WS_ENDPOINT_ADDRESS_TYPE" />.</para>
      <para>This header is not supported for <see cref="WS_ADDRESSING_VERSION_TRANSPORT" />.</para>
    </summary>
  </member>
</doc>