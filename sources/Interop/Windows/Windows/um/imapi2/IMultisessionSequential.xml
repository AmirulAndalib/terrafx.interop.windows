  <member name="IMultisessionSequential.get_FreeSectorsOnMedia">
    <summary>
      <para>Retrieves the number of free sectors available on the media.</para>
    </summary>
    <param name="value">
      <para>Number of sectors on the disc that are available for use.</para>
    </param>
