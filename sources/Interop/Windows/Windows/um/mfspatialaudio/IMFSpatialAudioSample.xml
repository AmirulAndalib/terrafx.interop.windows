<doc>
  <member name="IMFSpatialAudioSample.AddSpatialAudioObject">
    <summary>
      <para>Adds a new spatial audio object, represented by an <see cref="IMFSpatialAudioObjectBuffer" /> object, to the     sample.</para>
    </summary>
    <param name="pAudioObjBuffer">
      <para>A pointer to the new IMFSpatialAudioObject.</para>
    </param>
    <returns>
      <para>The method returns an <b>HRESULT</b>. Possible values include, but are not limited to, those in the following table.</para>
      <list type="table">
        <listheader>
          <description>Return code</description>
          <description>Description</description>
        </listheader>
        <item>
          <description>
            <para>
              <b>S_OK</b>
            </para>
          </description>
          <description>
            <para>The method succeeded.</para>
          </description>
        </item>
        <item>
          <description>
            <para>
              <b>E_POINTER</b>
            </para>
          </description>
          <description>
            <para>The supplied pointer is invalid.</para>
          </description>
        </item>
      </list>
    </returns>
    <seealso cref="IMFSpatialAudioSample" />
  </member>
  <member name="IMFSpatialAudioSample.GetObjectCount">
    <summary>
      <para>Gets the count of spatial audio objects, represented by <see cref="IMFSpatialAudioObjectBuffer" /> objects, in the sample.</para>
    </summary>
    <param name="pdwObjectCount">
      <para>A pointer to a 32 bit variable where the total number of audio objects in the sample will be stored.</para>
    </param>
    <returns>
      <para>The method returns an <b>HRESULT</b>. Possible values include, but are not limited to, those in the following table.</para>
      <list type="table">
        <listheader>
          <description>Return code</description>
          <description>Description</description>
        </listheader>
        <item>
          <description>
            <para>
              <b>S_OK</b>
            </para>
          </description>
          <description>
            <para>The method succeeded.</para>
          </description>
        </item>
        <item>
          <description>
            <para>
              <b>E_POINTER</b>
            </para>
          </description>
          <description>
            <para>The supplied pointer is invalid.</para>
          </description>
        </item>
      </list>
    </returns>
    <seealso cref="IMFSpatialAudioSample" />
  </member>
  <member name="IMFSpatialAudioSample.GetSpatialAudioObjectByIndex">
    <summary>
      <para>Returns the spatial audio object, represented by an <see cref="IMFSpatialAudioObjectBuffer" /> object, corresponding to the specified index.</para>
    </summary>
    <param name="dwIndex">
      <para>A 32 bit variable with the 0-based index of the requested audio object.</para>
    </param>
    <param name="ppAudioObjBuffer">
      <para>A pointer to an <see cref="IMFSpatialAudioObjectBuffer" /> object in which the spatial audio object corresponding with the specified index will be stored.</para>
    </param>
    <returns>
      <para>The method returns an <b>HRESULT</b>. Possible values include, but are not limited to, those in the following table.</para>
      <list type="table">
        <listheader>
          <description>Return code</description>
          <description>Description</description>
        </listheader>
        <item>
          <description>
            <para>
              <b>S_OK</b>
            </para>
          </description>
          <description>
            <para>The method succeeded.</para>
          </description>
        </item>
        <item>
          <description>
            <para>
              <b>E_POINTER</b>
            </para>
          </description>
          <description>
            <para>The supplied pointer is invalid.</para>
          </description>
        </item>
      </list>
    </returns>
    <seealso cref="IMFSpatialAudioSample" />
  </member>
  <member name="IMFSpatialAudioSample">
    <summary>
      <para>Represents a multimedia sample with spatial sound information. Every <b>IMFSpatialAudioSample</b> contains one or more <see cref="IMFSpatialAudioObjectBuffer" /> objects.</para>
    </summary>
    <seealso cref="IMFSample" />
  </member>
</doc>