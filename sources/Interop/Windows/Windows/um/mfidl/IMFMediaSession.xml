      <list type="table">
        <listheader>
          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>
                  <b>MFSESSIONCAP_PAUSE</b>
                </term>
              </listheader>
            </list>
          </description>
          <description>The Media Session can be paused.</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>
                  <b>MFSESSIONCAP_RATE_FORWARD</b>
                </term>
              </listheader>
            </list>
          </description>
          <description>The Media Session supports forward playback at rates faster than 1.0.</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>
                  <b>MFSESSIONCAP_RATE_REVERSE</b>
                </term>
              </listheader>
            </list>
          </description>
          <description>The Media Session supports reverse playback.</description>
        </listheader>
        <listheader>
