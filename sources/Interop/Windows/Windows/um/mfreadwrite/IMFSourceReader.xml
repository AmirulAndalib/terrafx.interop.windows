        <listheader>
          <description>Value</description>
          <description>Meaning</description>
        </listheader>
        <listheader>
          <description>
            <list type="bullet">
              <listheader>
                <term>0–0xFFFFFFFB</term>
              </listheader>
            </list>
          </description>
